.row
  .col-lg-12
    h1.page-header
      | Chart
      small  Visualization of logs
.row
  .col-md-3.col-md-push-9
    .row
      .col-md-12
        form.form.clearfix
          .form-group
            label Duration
            .input-group
              input.form-control#duration-start(type='text', placeholder='2016010113')
              span.input-group-addon to
              input.form-control#duration-end(type='text', placeholder='2016011019')
          button.btn.btn-success.pull-right#duration-btn(onclick='fetchData()') Apply
      .col-md-12
        svg#d3-map(width='100%', height='400px')
  .col-md-9.col-md-pull-3
    .row
      .col-md-4.info-col
        object(data='/static/img/thermometer.svg', type='image/svg+xml')
      .col-md-4.info-col
        object(data='/static/img/rain.svg', type='image/svg+xml')
      .col-md-4.info-col
      .col-md-12
script(src='/static/js/chart-script.js')
